    "helmet": "^3.22.0",
    "hogan.js": "^3.0.2",
    "ioredis": "^4.16.3",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "make-manifest": "^1.0.1",
    "on-headers": "^1.0.2",
    "optimist": "^0.6.1",
    "optional": "^0.1.4",
    "prepper": "^1.2.0",
    "ramda": "^0.27.0",
    "require-all": "^3.0.0",
