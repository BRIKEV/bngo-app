{
  "name": "bngo-app",
  "version": "0.0.1",
  "description": "bngo-app",
  "main": "index.js",
  "scripts": {
    "local": "SERVICE_ENV=local node index.js",
    "start": "NODE_ENV=production SERVICE_ENV=production node index.js",
    "test-ci": "SERVICE_ENV=test mocha --reporter mocha-junit-reporter",
    "test": "SERVICE_ENV=test mocha --reporter spec './test/**/*.test.js'",
    "lint": "eslint ./components ./lib ./test ./config",
    "manifest": "node_modules/make-manifest/bin/make-manifest",
    "qa": "npm run lint && npm run test",
    "precommit": "npm run lint",
    "build": "cd frontend && npm run build",
    "heroku-postbuild": "npm run build",
    "prepush": "npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.2.2",
    "expect.js": "^0.3.1",
    "husky": "^4.2.3",
    "make-manifest": "^1.0.1",
    "mocha": "^6.2.2",
    "mocha-junit-reporter": "^1.23.1",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "boom": "^7.3.0",
    "bunyan": "^1.8.12",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "confabulous": "^1.7.0",
    "debug": "^4.1.1",
    "error-handler-module": "^1.0.1",
    "express": "^4.17.1",
    "express-swagger-generator": "^1.1.15",
    "helmet": "^3.22.0",
    "hogan.js": "^3.0.2",
    "make-manifest": "^1.0.1",
    "on-headers": "^1.0.2",
    "optimist": "^0.6.1",
    "optional": "^0.1.4",
    "prepper": "^1.2.0",
    "ramda": "^0.26.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "swagger-endpoint-validator": "^1.1.5",
    "systemic": "^3.3.0",
    "systemic-domain-runner": "^1.1.0"
  }
}
